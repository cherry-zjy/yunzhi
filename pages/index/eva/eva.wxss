/* pages/index/eva/eva.wxss */
page{
  background-color: #F6F6F6;
}
textarea {
  font-size: 30rpx;
  width: 700rpx;
  padding: 25rpx 0;
}
.textarea-wrp {
  padding: 0 25rpx;
  background-color: #fff;
  margin-top: 20rpx;
}
.footer-btn {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  height: 98rpx;
  background: #26A3DE;
  opacity: 0.96;
  text-align: center;
}

.buy-btn {
  width: 118rpx;
  font-size: 30rpx;
  color: #fff;
  line-height: 98rpx;
}
/* 星级评价 */
.eva-star{
  float: left;
  width: 100%;
  border-top: 1rpx solid #F6F6F6;
  background-color: #fff;
  padding: 20rpx 0;
}
.title{
  float: left;
  font-size: 32rpx;
  margin-left: 25rpx;
}
.star-pos {
  float: right;
  margin-right: 25rpx;
}
.starsM{
    z-index: 1;
    width: 40rpx;
    height: 40rpx;
    background-image: url("data:image/png;base64,");
    background-size: 40rpx 40rpx;
    background-position: center center;
    background-repeat: no-repeat;
}
.star-pos .hideStar{
    z-index: 2;
    background-image: url("data:image/png;base64,");
    width: 40rpx;
    height: 40rpx;
    background-size: 40rpx 40rpx;
    background-position: center center;
    background-repeat: no-repeat;
}